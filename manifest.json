{
  "manifest_version": 2,

  "name": "Chuckles",
  "description": "Motivate yourself with a laugh from your favourite people",
  "version": "0.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click here for chuckles!"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}